{
    "name": "sumanthchilka/php-client-library",
    "description": "Client Library SDK for PHP",
    "type": "library",
    "homepage": "https://github.com/sumanthchilka/php-client-library",
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "ClientLibrary\\": "src"
        },
        "files": ["src/index.php"]
